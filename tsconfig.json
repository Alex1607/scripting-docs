{
  "compilerOptions": {
    "lib": ["es2018"],
    "target": "es6",
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "baseUrl": "./typings",
    "paths": {
      "@sinusbot": ["sinusbot/global.d.ts", "sinusbot/meta.d.ts"],
      "@sinusbot/*": ["interfaces/*"],
      "@sinusbot/modules/*": ["modules/*", "external/*"]
    }
  },
  "exclude": [
    "node_modules"
  ]
}