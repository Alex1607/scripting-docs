{
  "compilerOptions": {
    "allowJs": true,
    "checkJs": true,
    "module": "commonjs",
    "target": "es6",
    "baseUrl": "./typings",
    "paths": {
      "@sinusbot": ["sinusbot/sinusbot.d.ts", "sinusbot/meta.d.ts"],
      "@sinusbot/*": ["interfaces/*"]
    },
    "typeRoots" : ["./typings"],
    "types": ["sinusbot"]
  },
  "exclude": [
    "node_modules",
    "typings"
  ]
}