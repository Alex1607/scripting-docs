{
  "compilerOptions": {
    "lib": ["es2018"],
    "module": "commonjs",
    "target": "es6",
    "noImplicitAny": true,
    "strictNullChecks": true,
    "baseUrl": "./typings",
    "paths": {
      "@sinusbot": ["sinusbot/sinusbot.d.ts", "sinusbot/meta.d.ts"],
      "@sinusbot/*": ["interfaces/*"],
      "@sinusbot/modules/*": ["modules/*", "external/*"]
    },
    "typeRoots" : ["./typings"],
    "types": ["sinusbot"]
  },
  "exclude": [
    "node_modules",
    "typings"
  ]
}